{
  "version": 2,
  "routes": [
    { "src": "^/$", "dest": "/static/index.html" },

    { "src": "^/static/(.*)$", "dest": "/static/$1" },

    {
      "src": "^/api/(.*)$",
      "dest": "https://studytrackerog.onrender.com/api/$1"
    },

    { "src": "^(?!/api/).*", "dest": "/static/index.html" }
  ]
}
